        <name>Automatically Populate Total price</name>
        <order/>
        <script><![CDATA[function onLoad() {
   //Type appropriate comment here, and begin script below
	var ticket = new GlideRecord('x_844144_movie_man_ticket');
	var booking = new GlideRecord('x_844144_movie_man_booking');
	ticket.addActiveQuery();
	ticket.setLimit(1);
	ticket.query();
	while (ticket.next()){
		booking.total_price = ticket.no_of_tickets * ticket.price;
	}
   
}]]></script>
